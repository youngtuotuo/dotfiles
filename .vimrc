syntax on
set tabstop=2
set shiftwidth=2
set expandtab
set ai
set number
set hlsearch
set ruler
set rnu
set nu
set background=dark
hi CursorLine guibg=235 ctermbg=235
hi CursorLineNr guifg=11 ctermfg=11
hi Visual guifg=NONE ctermfg=NONE guibg=238 ctermbg=238
hi Comment guifg=DarkGrey ctermfg=DarkGrey
hi Pmenu guibg=239 ctermbg=239
hi PmenuSel guifg=Black guibg=DarkGrey ctermfg=232 ctermbg=254
hi NormalFloat guibg=NONE ctermbg=NONE
hi FloatBorder guibg=NONE ctermbg=NONE
hi StatusLineNC gui=reverse cterm=reverse
hi! link MatchParen Visual
hi VertSplit cterm=reverse gui=reverse
hi netrwMarkFile ctermfg=209 guifg=209
